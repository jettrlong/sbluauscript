    for i,v in pairs(workspace:GetDescendants()) do
        if v.Name == "End" and v.ClassName == "Part" then
            v.Size = Vector3.new(28, 0.3, 4)
        end
    end
    while AutoTycoon do
        for i,v in pairs(workspace:GetDescendants()) do
            if v.Name == "Click" and v:FindFirstChild("ClickDetector") then
                fireclickdetector(v.ClickDetector)
            end
        end
        task.wait()
    end
end)


